{
  "name": "clean-architecture",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create:local": "NODE_ENV=local npm run typeorm:cli -- migration:create ./src/migrations/local/$npm_config_name",
    "makemigrations:local": "NODE_ENV=local npm run typeorm:cli -- -d ./src/infrastructure/database/typeorm/typeorm.options.ts migration:generate ./database/migrations/local/%npm_config_name%",
    "makemigrations:local:mac": "NODE_ENV=local npm run typeorm:cli -- -d ./src/infrastructure/database/typeorm/typeorm.options.ts migration:generate ./database/migrations/local/$npm_config_name",
    "makemigrations:dev": "NODE_ENV=dev npm run typeorm:cli -- -d ./src/infrastructure/database/typeorm/typeorm.options.ts migration:generate ./database/migrations/dev/%npm_config_name%",
    "makemigrations:prod": "NODE_ENV=prod run typeorm:cli -- -d ./src/infrastructure/database/typeorm/typeorm.options.ts migration:generate ./database/migrations/prod/%npm_config_name%",
    "migrations:local": "NODE_ENV=local npm run typeorm:cli migration:run -- -d ./src/infrastructure/database/typeorm/typeorm.options.ts",
    "migrations:dev": "NODE_ENV=local npm run typeorm:cli migration:run -- -d ./src/infrastructure/database/typeorm/typeorm.options.ts",
    "migrations:prod": "NODE_ENV=prod npm run typeorm:cli migration:run -- -d ./src/infrastructure/database/typeorm/typeorm.options.ts",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:cov": "NODE_ENV=local jest --config jest.config.js --coverage",
    "test:debug": "NODE_ENV=local node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.15",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.12",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.15",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@ocoda/event-sourcing": "^2.1.3",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "nestjs-bustransit": "^0.1.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "shallow-equal-object": "^1.1.1",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.12",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-extended": "^6.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
